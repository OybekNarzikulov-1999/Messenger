# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Messenger' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Messenger

	# Firebase

	pod 'SnapKit', '~> 5.0.0'
	pod 'FirebaseDatabase'
	pod 'FirebaseAuth'
	pod 'FirebaseStorage'

  	# Facebook
	pod 'FBSDKLoginKit'

	# Google
	pod 'GoogleSignIn'

	pod 'MessageKit'
	pod 'JGProgressHUD'
	pod 'SDWebImage'

end

